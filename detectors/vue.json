{  
  "html": "<[^>]+\\sdata-v(?:ue)?-",
  "js": "window.Vue || window.$nuxt || [...document.querySelectorAll('*')].map((el) => Boolean(el.__vue__)).filter(Boolean).length",
  "script": [
    "vue[.-]([\\d.]*\\d)[^/]*\\.js\\;version:\\1",
    "(?:/([\\d.]+))?/vue(?:\\.min)?\\.js\\;version:\\1"
  ]
}
